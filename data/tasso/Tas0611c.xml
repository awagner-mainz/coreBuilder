    <text>
        <body xml:space="preserve">
            <lg>
                <l xml:id="l1">La bella pargoletta</l>
                <l xml:id="l2"><seg xml:id="v1">Ch'ancor non sente</seg> Amore</l>
                <l xml:id="l3">Ne <seg xml:id="v2">pur not'ha per fama</seg> il suo valore</l>
                <l xml:id="l4"><seg xml:id="v3">Col bel guardo</seg> saetta</l>
                <l xml:id="l5">E col <seg xml:id="v4">soave</seg> riso</l>
                <l xml:id="l6">Ne <seg xml:id="v5">s’accorge che l’arm'ha nel bel</seg> viso</l>
                <l xml:id="l7"><seg xml:id="v6">Che</seg> colp'ha <seg xml:id="v7">del</seg> morire</l>
                <l xml:id="l8">Della trafitta gente</l>
                <l xml:id="l9">S'ei non sa di ferire</l>
                <l xml:id="l10">O <seg xml:id="v7">bellezza homicida</seg> &amp; innocente</l>
                <l xml:id="l11">Temp'<seg xml:id="v8">e ch'amor ti mostri</seg></l>
                <l xml:id="l12"><seg xml:id="v9">Homai nelle tue piagh'i dolor</seg> nostri.</l>
            </lg>
        </body>
    </text>
