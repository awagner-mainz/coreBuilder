<surface xmlns="http://www.tei-c.org/ns/1.0" xmlns:mith="http://mith.umd.edu/sc/ns1#" ulx="0"
  uly="0" lrx="5406" lry="7276" xml:id="ox-ms_shelley_e1-0010" partOf="#ox-ms_shelley_e1"   mith:folio="4v" mith:shelfmark="MS. Shelley e. 1">
  <graphic url="http://shelleygodwinarchive.org/images/ox/ox-ms_shelley_e1-0010.jp2"/>
  <zone type="main">
    <milestone unit="tei:speaker" spanTo="#e1-0010.01"/>
    <line xml:id="l0" rend="center">Chorus of Spirits</line>
    <anchor xml:id="e1-0010.01"/>
    <line xml:id="l1" rend="indent1">We join the throng</line>
    <line xml:id="l2" rend="indent1">Of the dance &amp; the song</line>
    <line xml:id="l3">By the whirlwind of gladness borne along;</line>
    <line xml:id="l4" rend="indent1">As the flying-fish leap</line>
    <line xml:id="l5" rend="indent1">From the Indian deep,</line>
    <line xml:id="l6">And mix with the sea birds half asleep.</line>
    <space dim="vertical" unit="lines" extent="1" />
    <milestone unit="tei:speaker" spanTo="#e1-0010.02"/>
    <line xml:id="l7" rend="center">Chorus of Hours</line>
    <anchor xml:id="e1-0010.02"/>
    <line xml:id="l8">Whence come<del rend="strikethrough">s</del> ye so wild &amp; so fleet</line>
    <line xml:id="l9">For sandals of lightning are on your feet</line>
    <line xml:id="l10">And your wings are soft &amp; swift as thought,</line>
    <line xml:id="l11">And your eyes are as Love which is veiled not<unclear reason="illegible"
      >?</unclear></line>
    <space dim="vertical" unit="lines" extent="1" />
    <milestone unit="tei:speaker" spanTo="#e1-0010.03"/>
    <line xml:id="l12" rend="center">Chorus of spirits</line>
    <anchor xml:id="e1-0010.03"/>
    <line xml:id="l13" rend="indent1">We come from the mind</line>
    <line xml:id="l14" rend="indent1">Of human kind</line>
    <line xml:id="l15">Which was late so dusk &amp; obscene &amp; blind<unclear reason="illegible"
      >;</unclear></line>
    <line xml:id="l16" rend="indent1">Now tis an <unclear reason="case">O</unclear>cean,</line>
    <line xml:id="l17" rend="indent1">Of clear emotion</line>
    <line xml:id="l18"><mod>
        <del rend="overwritten">Of</del>
        <add place="intralinear">A </add>
      </mod>
      <unclear reason="case">H</unclear>eaven of serene &amp; mighty motion.</line>
    <space dim="vertical" unit="lines" extent="1" />
    <line xml:id="l19" rend="indent1">From that deep Abyss</line>
    <line xml:id="l20" rend="indent1">Of wonder &amp; bliss</line>
    <line>Whose caverns are chrystal palaces,</line>
  </zone>
</surface>
